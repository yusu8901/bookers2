<div class="container">
  <div class="row">
    <%= render 'books/list', book_new: @book_new, user: @user %>
    <div class="offset-md-1 col-md-8">
      <h1>Users</h1>
      <table>
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <div>
              <td><p><%= image_tag user.get_profile_image(100,100) %></p></td>

              <td><p><%= user.name %></p></td>
              <td><%= link_to 'Show', user_path(user) , class: 'nav-link' %></td>
            </div>
          </tr>
          <% end %>

        </tbody>
      </table>


    </div>
  </div>
</div>